{
    "databaseChangeLog": [
        {
            "changeSet": {
              "id": "Create park_location table",
              "author": "ahmoh",
              "changes": [
                {
                  "createTable": {
                    "tableName": "park_location",
                    "columns": [
                      {
                        "column": {
                          "name": "id",
                          "type": "uuid",
                          "constraints": {
                            "primaryKey": true,
                            "nullable": false
                          }
                        }
                      },
                      {
                        "column": {
                          "name": "car_id",
                          "type": "uuid",
                          "constraints": {
                            "foreignKeyName": "fk_car_id",
                            "referencedTableName": "cars",
                            "referencedColumnNames": "id",
                            "nullable": false
                          }
                        }
                      },
                      {
                        "column": {
                          "name": "park_start_time",
